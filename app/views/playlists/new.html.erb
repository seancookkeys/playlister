<div class="booyah-box col-10 offset-1">
    <h1>Add Your Playlist Here</h1>
    <%= simple_form_for @playlist do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :link %>
        <br />
        <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>